  <!-- google map js -->
  {{#if map}}
  <script>
    let locations = [];
    let agencyId = $('#agencyId').val();
    let url = `http://localhost:3000/location/vehicle/${agencyId}`;
    function initialize() {
      var map = new google.maps.Map(document.getElementById('map'), {
        zoom: 8,
        center: new google.maps.LatLng(19.0760, 72.8777),
        mapTypeId: google.maps.MapTypeId.ROADMAP
      });

      $.ajax({
        type: "GET",
        url: url,
        success: function (data) {
          let l = data.length;

          for (i = 0; i < l; i++) {
            let latlng = data[i];
            locations.push(latlng);
          }

          var infowindow = new google.maps.InfoWindow();
          var marker, i;

          for (i = 0; i < locations.length; i++) {
            marker = new google.maps.Marker({
              position: new google.maps.LatLng(locations[i].lat, locations[i].lng),
              map: map
            });

            google.maps.event.addListener(marker, 'click', (function (marker, i) {
              return function () {
                infowindow.setContent(locations[i]._id);
                infowindow.open(map, marker);
              }
            })(marker, i));
          }

        },
        error: function () {
          alert(" Error as occurred while sending your location please trying refresh your page");
        }
      });


    }
  </script>
  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCwpIltCnAhMK3vY7J6g9b7uEYX-Su3wo0&callback=initialize">
  </script>
  {{/if}}
