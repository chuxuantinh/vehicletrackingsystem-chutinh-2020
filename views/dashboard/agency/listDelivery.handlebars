                  <!--        Here you can write extra buttons/actions for the toolbar              -->
                </div>
                <table id="datatable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                  <thead>
                    <tr>
                      <th>Agency ID</th>
                      <th>Vehicle Type</th>
                      <th>Vehicle Number</th>
                      <th>Vehicle Capacity</th>
                      <th>Vehicle Brand</th>
                      <th>Location Arrived</th>
                      <th>Location Reached</th>
                      <th>Status</th>
                    </tr>
                  </thead>
                  <tfoot>
                    <tr>
                      <th>Driver ID</th>
                      <th>Vehicle Type</th>
                      <th>Vehicle Number</th>
                      <th>Vehicle Capacity</th>
                      <th>Vehicle Brand</th>
                      <th>Location Arrived</th>
                      <th>Location Reached</th>
                      <th>Status</th>
                    </tr>
                  </tfoot>
                  <tbody>
                    {{#each delivery}}
                    <tr>
                      <td>{{ this.driverId }}</td>
                      <td>{{ this.vType }}</td>
                      <td>{{ this.vNumber }}</td>
                      <td>{{ this.vCapacity }}</td>
                      <td>{{ this.vBrand }}</td>
                      <td>{{ this.fLocation }}</td>
                      <td>{{ this.tLocation }}</td>

                      {{#if this.isCompleted}}
                        <td><button class="btn btn-success">Completed</button></td>
                      {{else}}
                        <td><button class="btn btn-danger">Incompleted</button><br>
                          <div>
                            <form action="/delivery/mark/{{this._id}}?_method=PUT" method="POST">
                            <input type="hidden" name="_method" value="PUT">
                              <button class="btn btn-primary btn-link" type="submit">mark completed</button>
                            </form>
                          </div>
                        </td>
                      {{/if}}
                      
                    </tr>
                    {{/each}}
                  </tbody>
                </table>
              </div>
              <!-- end content-->
            </div>
            <!--  end card  -->
          </div>
          <!-- end col-md-12 -->
        </div>
        <!-- end row -->
        {{else}}
        <div class="alert alert-info">
            <span>Your agency does not created delivery</span>
        </div>
        {{/if}}
